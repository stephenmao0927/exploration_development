<?xml version="1.0"?>
<!-- 此文件单独测试detector使用 -->
<launch>
  <!-- 启动地图 -->
  <node pkg="new_detector" type="map_1.py" name="map"  output="screen"/>
  <!-- 启动odom发布 -->
  <node pkg="new_detector" type="odom_test.py" name="odom" output="screen"/>
  <!-- 启动detector -->
  <node pkg="new_detector" type="detector.py" name="detector" output="screen">
    <param name="costmap_topic" value="map"/>
    <param name="odometry_topic" value="/robot_1/odom"/>
    <param name="radius" value="30"/>
    <param name="number" value="30"/>
  </node>

  <!--启动rrt全局规划器-->
  <!-- <node pkg="rrt_star_global_planner" type="rrt_star_planner" respawn="false" name="rrt_star_planner" output="screen">
    <rosparam file="$(find rrt_star_global_planner)/params/test_rrt_star_planner.yaml" command="load" />
  </node> -->

  <node pkg="new_detector" type="filter_test.py" name="filter" output="screen">
  <param name="map_topic" value="map"/>
  <param name="info_radius" value="1"/> 
  <param name="costmap_clearing_threshold" value="70"/> 
  <param name="goals_topic" value="/detected_points"/>
  <param name="namespace" value=""/> 
  <param name="n_robots" value="1"/>
  <param name="rate" value="100"/>
  <param name="global_costmap_topic" value="map"/>

  </node>

  <node pkg="rviz" type="rviz" name="rviz" required="true"
          args="-d $(find new_detector)/src/rviz/test.rviz"/>

</launch>


